{
  "name": "cloudscraper-ts",
  "version": "1.0.0",
  "description": "A TypeScript port of the cloudscraper Python library to bypass Cloudflare's anti-bot page",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepare": "npm run build",
    "check-tests": "tsc -p tsconfig.test.json --noEmit"
  },
  "keywords": [
    "cloudflare",
    "scraper",
    "bypass",
    "antibot",
    "captcha",
    "challenge",
    "typescript"
  ],
  "author": "Converted from Python cloudscraper by VeNoMouS",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "axios-cookiejar-support": "^4.0.7",
    "brotli": "^1.3.3",
    "cheerio": "^1.0.0-rc.12",
    "node-fetch": "^3.3.2",
    "puppeteer": "^21.5.2",
    "tough-cookie": "^4.1.3",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/brotli": "^1.3.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.4",
    "@types/tough-cookie": "^4.0.5",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "axios-mock-adapter": "^1.22.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "nock": "^13.4.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  }
}
